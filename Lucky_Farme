        function toggleFilterForm() {
            var filterForm = document.getElementById('filterForm');
            if (filterForm.style.display === "none") {
                filterForm.style.display = "block";
            } else {
                filterForm.style.display = "none";
            }
        }

        function applyFilter() {
            var category = document.getElementById('category').value;
            var price = document.getElementById('price').value;

            var filteredProducts = products.filter(product => 
                (!category || product.category === category) &&
                (!price || product.price <= Number(price))
            );

            displayProducts(filteredProducts);
        }

        function addProduct() {
            var name = document.getElementById('productInput').value;
            var newProduct = { name: name, description: 'Описание товара', price: 1000, category: 'Категория' };
            products.push(newProduct);
            displayProducts(products);
        }

function addToCart(name, description, price) {
    cart.addItem({name: name, description: description, price: price});
}

function displayProducts(products) {
    var productsElement = document.getElementById('products');
    productsElement.innerHTML = '';

    products.forEach(product => {
        var productElement = document.createElement('div');
        productElement.innerHTML = `<p>${product.name}</p><p>${product.description}</p><p>${product.price} тенге</p><p>${product.category}</p><button onclick='cart.addItem(${JSON.stringify(product)})'>Добавить в корзину</button>`;
        productsElement.appendChild(productElement);
    });
}


var avatars = [
    'https://i.ibb.co.com/py4m4bC/fruits-6357256-640.jpg',
    'https://i.ibb.co.com/LNZv8hd/plant-8432181-640.jpg',
    'https://i.ibb.co.com/MpCGW8c/raspberries-5576401-640.jpg',
    'https://cdn.pixabay.com/photo/2018/05/26/10/54/strawberries-3431122_640.jpg',
    'https://cdn.pixabay.com/photo/2023/08/15/14/36/fruit-8192071_640.jpg'
];

function registerUser() {
    var name = document.getElementById('name').value;
    var login = document.getElementById('login').value;
    var password = document.getElementById('password').value;

    if (name && login && password) {
        var existingUser = JSON.parse(localStorage.getItem('userData'));
        if (existingUser && existingUser.login === login) {
            alert('Такой пользователь уже существует!');
            return;
        }

        if (password.length < 8) {
            alert('Пароль должен содержать более 8 символов!');
            return;
        }

        var user = {
            name: name,
            login: login,
            password: password,
            avatar: avatars[Math.floor(Math.random() * avatars.length)],
            cart: cart.items
        };

if (name === 'Удачная Ферма' && login === 'Admin321' && password === '123_YF_123') {
    user.isAdmin = true;
    user.avatar = 'https://i.ibb.co.com/P18BBG6/image.jpg'; // Укажите путь к изображению аватара администратора
    alert('Добро пожаловать, администратор!');
} else {
    user.avatar = 'https://i.ibb.co.com/P18BBG6/image.jpg'; // Укажите путь к изображению аватара по умолчанию
}

        localStorage.setItem('userData', JSON.stringify(user));
        updateProfile();

        document.getElementById('name').value = '';
        document.getElementById('login').value = '';
        document.getElementById('password').value = '';

        alert('Пользователь зарегистрирован:', name, login, password);
    } else {
        alert('Все поля должны быть заполнены!');
    }
}

function loadCartFromServer() {
    var userData = JSON.parse(localStorage.getItem('userData'));
    if (userData) {
        cart.items = userData.cart; // Загружаем товары в корзину из данных пользователя
        cart.updateCartUI();
    }
}

function toggleForm() {
    var form = document.getElementById('registrationForm');
    if (form.style.display === "none") {
        form.style.display = "block";
    } else {
        form.style.display = "none";
    }
}

try {
    function updateProfile() {
        var userData = JSON.parse(localStorage.getItem('userData'));
        if (userData) {
            document.getElementById('username').textContent = userData.name;
            var avatarElement = document.getElementById('avatar');
            avatarElement.src = userData.avatar;

            avatarElement.addEventListener('click', function() {
                alert('Имя: ' + userData.name + '\nЛогин: ' + userData.login + '\nПороль: ' + userData.password);
            });
        }
    }

window.onload = function() {
    var nameInput = document.getElementById('name');
    var loginInput = document.getElementById('login');
    var passwordInput = document.getElementById('password');
    var submitButton = document.getElementById('submit');

    nameInput.addEventListener('input', function() {
        console.log('Имя: ' + this.value);
    });

    loginInput.addEventListener('input', function() {
        console.log('Логин: ' + this.value);
    });

    passwordInput.addEventListener('input', function() {
        console.log('Пароль: ' + this.value);
    });

    submitButton.addEventListener('click', function() {
        registerUser();
    });
}; 

    document.getElementById('avatar').addEventListener('click', function() {
        var userData = JSON.parse(localStorage.getItem('userData'));
        if (userData) {
            alert('Имя: ' + userData.name + '\nЛогин: ' + userData.login + '\nПороль: ' + userData.password);
        }
    });

} catch (error) {
    console.log('Произошла ошибка в основном скрипте:', error);
}

window.onload = function() {
// Массив для хранения отзывов
let reviews = [
    "Отличный продукт! Очень доволен покупкой.",
    "Быстрая доставка, товар соответствует описанию.",
    "Продавец был очень вежлив и помог мне с выбором.",
    // добавьте больше отзывов по мере необходимости
];

let reviewInput = document.getElementById('reviewInput');
let submitReviewButton = document.getElementById('submitReviewButton');
let reviewsContainer = document.getElementById('reviewsContainer');

submitReviewButton.addEventListener('click', function() {
    let reviewText = reviewInput.value.trim();

    if (reviewText) {
        reviews.push(reviewText);

        reviewInput.value = '';

        updateReviewsDisplay();
    } else {
        alert('Пожалуйста, введите отзыв.');
    }
});

function updateReviewsDisplay() {
    reviewsContainer.innerHTML = '';

    reviews.forEach(function(review, index) {
        let reviewElement = document.createElement('p');
        reviewElement.textContent = `Отзыв ${index + 1}: ${review}`;
        reviewsContainer.appendChild(reviewElement);
    });
}
}
// Вызываем функцию для отображения отзывов при загрузке страницы
updateReviewsDisplay();
</script>
