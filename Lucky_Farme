<script>
            window.onload = function() {
            document.getElementById('myElement').style.animationPlayState = 'running';
        };

var cart = {
    items: [],
    addItem: function(item) {

        var existingItem = this.items.find(function(i) {
            return i.name === item.name;
        });
        if (existingItem) {
            alert('Товар уже в корзине');
        } else {
            this.items.push(item);
            this.updateCartUI();
            this.saveCartToLocalStorage();
        }
    },
    removeItem: function(index) {
        this.items.splice(index, 1);
        this.updateCartUI();
        this.saveCartToLocalStorage();
    },
    clearCart: function() {
        this.items = [];
        this.updateCartUI();
        this.saveCartToLocalStorage();
    },
    saveCartToLocalStorage: function() {
        localStorage.setItem('cart', JSON.stringify(this.items));
    },
    loadCartFromLocalStorage: function() {
        this.items = JSON.parse(localStorage.getItem('cart')) || [];
    },
    toggleCart: function() {
        var cartElement = document.getElementById('cart');
        cartElement.style.display = cartElement.style.display === 'none' ? '' : 'none';
    },
    updateCartUI: function() {
        var cartElement = document.getElementById('cart');
        cartElement.innerHTML = '';
        this.items.forEach(function(item, index) {
            cartElement.innerHTML += '<p>Название: ' + item.name + '</p>';
            cartElement.innerHTML += '<p>Описание: ' + item.description + '</p>';
            cartElement.innerHTML += '<p>Цена: ' + item.price + ' тенге</p>';
            cartElement.innerHTML += '<button onclick="cart.removeItem(' + index + ')">Удалить</button><br><br>';
            var message = 'Здравствуйте, хочу заказать ' + item.name;
            cartElement.innerHTML += '<a href="https://wa.me/+77479750816?text=' + encodeURIComponent(message) + '" target="_blank"><button>Оформить покупку</button></a><br><br>';
        });
        cartElement.innerHTML += '<button onclick="cart.clearCart()">Удалить всё</button><br><br>';
    }
};

window.onload = function() {
    cart.loadCartFromLocalStorage();
};

var products = [
            { name: 'Альба', description: 'Сорт клубники "Альба"', price: 500 },
            { name: 'Дюк', description: 'Сорт голубики "Дюк"', price: 6000 },
            { name: 'Августин', description: 'Сорт винограда "Августин"', price: 1300 },
            { name: 'Осейдж', description: 'Сорт еживики "Осейдж"', price: 1500 },
            { name: 'Чудо', description: 'Сорт вишни "Чудо"', price: 1800 },
            { name: 'Царский', description: 'Сорт абрикос "Царский"', price: 1800 },
            { name: 'Ираморная', description: 'Сорт груши "Мраморная"', price: 1800 },
            { name: 'Ламарка', description: 'Сорт ирги "Ламарка"', price: 2200 },
            { name: 'Бен Лир', description: 'Сорт Клюква "Бен Лир"', price: 2000 }
];

function addToCart(name, description, price) {
    var item = {name: name, description: description, price: price};
    var cart = getCartData();
    cart.push(item);

    // Сохраняем обновленные данные корзины в localStorage
    localStorage.setItem('cart', JSON.stringify(cart));
}

// Функция для обновления корзины
function updateCart() {
    var cart = getCartData();

    // Здесь должен быть ваш код для обновления данных корзины
    updateCartUI(cart);

    // Сохраняем обновленные данные корзины в localStorage
    localStorage.setItem('cart', JSON.stringify(cart));
}

var searchHistory = [];

function searchProducts() {
    var searchInput = document.getElementById('searchInput').value;
    var searchResults = products.filter(product => product.name.includes(searchInput));

    // Add the search results to the search history
    searchHistory.push(...searchResults);

    var searchResultsElement = document.getElementById('searchResults');
    searchResultsElement.innerHTML = '';

    searchResults.forEach((product, index) => {
        var productElement = document.createElement('div');
        productElement.innerHTML = `<p>${product.name}</p><p>${product.description}</p><p>${product.price} тенге</p><p>${product.category}</p><button onclick='cart.addItem(${JSON.stringify(product)})'>Добавить в корзину</button>`;
        searchResultsElement.appendChild(productElement);
    });

    // Add the "Hide Search History" button
    var hideButton = document.createElement('button');
    hideButton.onclick = hideSearchHistory;
    hideButton.textContent = 'Скрыть историю поиска';
    searchResultsElement.appendChild(hideButton);
}

function hideSearchHistory() {
    searchHistory = [];
    document.getElementById('searchResults').innerHTML = '';
}

function hideFromSearchHistory(index) {
    searchHistory[index].hidden = true;
    // Re-render the search results
    searchProducts();
}

function deleteFromSearchHistory(index) {
    searchHistory.splice(index, 1);
    // Re-render the search results
    searchProducts();
}

function toggleSettings() {
    var settingsElement = document.getElementById('settings');
    settingsElement.style.display = settingsElement.style.display === 'none' ? 'block' : 'none';
}

function changeColor(color) {
    document.body.style.backgroundColor = color;
}

